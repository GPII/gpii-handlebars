        "md5": "2.2.1",
        "underscore-node": "0.1.2"
    },
    "devDependencies": {
        "cheerio": "1.0.0-rc.3",
        "eslint": "6.4.0",
        "eslint-config-fluid": "1.3.0",
        "gpii-grunt-lint-all": "1.0.5",
        "gpii-testem": "2.1.10",
        "graceful-fs": "4.2.2",
        "grunt": "1.0.4",
        "kettle": "1.11.0",
        "mkdirp": "0.5.1",
        "node-jqunit": "1.1.8",
